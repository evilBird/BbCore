GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/BbRuntime" "${PODS_ROOT}/Headers/Public/UIView+Layout"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/BbRuntime" -isystem "${PODS_ROOT}/Headers/Public/UIView+Layout"
OTHER_LDFLAGS = $(inherited) -ObjC -l"BbRuntime" -l"UIView+Layout" -framework "AVFoundation" -framework "CoreLocation" -framework "Foundation" -framework "MapKit" -framework "UIKit"
PODS_ROOT = ${SRCROOT}/Pods